    "@popperjs/core": "^2.9.3",
    "@svgr/webpack": "4.3.3",
    "@tailwindcss/forms": "^0.3.3",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "apollo-boost": "^0.4.9",
    "axios": "^0.21.1",
    "babel-eslint": "10.1.0",
